programa
{
	
	funcao inicio()
	{
		
		inteiro vetor[10]
		
		para(inteiro i=0; i < 10; i++){
		escreva("Digite um número inteiro: ")
		leia(vetor[i])
		}

		para(inteiro i=9; i >= 0; i--){
		escreva(vetor[i] + " ")
		}
		
	}
}
